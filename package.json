{
    "name": "techlab-ide-native",
    "version": "0.4.0",
    "description": "TODO",
    "main": "app.js",
    "window": {
        "title": "TechLabIDE",
        "fullscreen": true,
        "position": "center"
    },
    "repository": {
        "type": "git",
        "url": "https//github.com/BardinPetr/TechLabIDE_ElectronApp.git"
    },
    "keywords": [
        "arduino"
    ],
    "author": "Bardin Petr <bardin.petr@gmail.com>",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/BardinPetr/TechLabIDE_ElectronApp/issues"
    },
    "homepage": "https://github.com/BardinPetr/TechLabIDE_ElectronApp",
    "dependencies": {
        "avrgirl-arduino": "2.1.0",
        "electron": "^1.6.8",
        "electron-prebuilt": "^1.4.13",
        "electron-window-manager": "^1.0.4",
        "path": "^0.12.7",
        "serialport": "^4.0.7"
    },
    "devDependencies": {
        "electron-rebuild": "^1.5.11",
        "electron_packager": "^1.0.5"
    },
    "scripts": {
        "install": "npm run spRebuild",
        "test": "echo \"Error: no test specified\"",
        "start": "electron .",
        "buildWin": "electron-packager . TechLabIDE --icon /app/media/icons/icon64.ico --platform=win32 --arch=x64 --electron-version=1.6.8 --app-version=0.3.0 --out=dist --prune --overwrite --win32metadata.CompanyName=\"RoboNEL\" --win32metadata.ProductName=\"TechLabIDE-Visual programming IDE\" --win32metadata.requestedExecutionLevel=\"asInvoker\"",
        "buildMac": "electron-packager . TechLabIDE --icon /app/media/icons/icon64.icns --platform=darvin --arch=all --electron-version=1.6.8 --app-version=0.3.0 --out=dist --prune --overwrite && electron-packager . TechLabIDE --icon /app/media/icons/icon64.icns --platform=mas --arch=all --electron-version=1.6.8 --app-version=0.3.0 --out=dist --prune --overwrite",
        "buildLin": "electron-packager . TechLabIDE --icon /app/media/icons/icon64.png --platform=linux --arch=x64 --electron-version=1.6.8 --app-version=0.3.0 --out=dist --prune --overwrite",
        "build": "npm run buildWin && npm run buildMac && npm run buildLin",
        "spRebuild": "cd /node_modules/serialport &&  HOME=~/.electron-gyp node-gyp rebuild --target=1.4.13 --arch=x64 --dist-url=https://atom.io/download/electron"
    }
}